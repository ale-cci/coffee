import "./std/testing" as testing
import "./token" as token

int main() {
    t := testing.init("token.test.bn")

    testing.setup(t, "does not parse invalid <null> token pointer")
    testing.run(t, should_not_parse_null_token_name(t))

    return 0
}

int should_not_parse_null_token_name(testing.T* t) {
    token_id := token.id((chr*) null)
    testing.assert_equal_int(t, 0, token_id)
    return 0
}
